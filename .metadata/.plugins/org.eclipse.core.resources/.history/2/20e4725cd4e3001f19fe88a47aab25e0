package Day10;

import java.nio.channels.Pipe.SourceChannel;

public class InbuiltMethodInString4 {

	public static void main(String[] args) {
		// replace()--> It will replace single/multiple character in a string..
		
		// For single char
		String str1="JavaProgrammingLanuage";
		System.out.println(str1);
		System.out.println(str1.replace('a', 'B'));
		
		//For Multiple;
		String str2="Java Selenuim with Mavan";
		System.out.println(str2);
		System.out.println(str2.replace("Java", "Pyrhon"));

	}

}
