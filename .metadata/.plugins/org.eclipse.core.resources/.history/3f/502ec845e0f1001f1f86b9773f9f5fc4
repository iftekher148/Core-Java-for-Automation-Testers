package Day15;

public class HelloMain {

	public static void main(String[] args) {
		
		// create object of "Hello" class
		Hello h1=new Hello();
		
		//calling the method
		h1.Message1();
		
		//System.out.println(h1.Message2());
		int x= h1.Message2(); // store the return value into a variable
		System.out.println(x);
		
		h1.Message3("Rocktim");
		h1.Message4("CSE", 12);
	}

}
