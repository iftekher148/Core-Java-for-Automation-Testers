package Day23;


public class PrivateTestClass {

	//Declare private variable and method
	private int a =100;
	private void method1(){
		System.out.println("Default Method");
	}
	public static void main(String[] args) {
		
		PrivateTestClass obj = new PrivateTestClass();
		System.out.println(obj.a); // private methods and variable can only access within the same class. Not outside of the other class
		obj.method1();
	}

}
