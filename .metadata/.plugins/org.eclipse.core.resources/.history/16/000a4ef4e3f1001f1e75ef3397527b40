package Day15;

public class Employee {
	
	//Declare variable
	int empID;
	String empName;
	double empSalary;
	
	//User defined method
	/*
	void employeeDetails() {
		System.out.println(empID+" "+empName+" "+empSalary);
	}
	*/
	void getEmployeeData(int id, String name, double salary) {
		empID=id;
		empName=name;
		empSalary=salary;
	}
}
