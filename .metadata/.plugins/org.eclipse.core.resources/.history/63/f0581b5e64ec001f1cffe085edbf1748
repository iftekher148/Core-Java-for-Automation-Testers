package Day12;

public class RemoveJunkSpecialCharacter {

	public static void main(String[] args) {
		String str1= "&&8)&$Java(*^$Programming)(&^%%Language"; //input string with junk & special charcters;
		
		String str2=str1.replaceAll("[a-zA-z0-9]", ""); // This will remove all latter and digit 
		System.out.println(str2);

	}

}
